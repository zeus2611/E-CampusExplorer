<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/referAndEarnLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"

    tools:context=".referAndEarn">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        tools:context=".MainActivity">


        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/poppins"
            android:text="@string/refer_and_earn"
            android:textColor="@color/black"
            android:textFontWeight="500"
            android:textSize="19sp"
            app:layout_constraintBottom_toTopOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <Button
            android:id="@+id/button23"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="12dp"
            android:layout_marginBottom="22dp"
            android:background="@android:color/transparent"
            android:drawableLeft="@drawable/baseline_arrow_back_24"
            app:layout_constraintBottom_toTopOf="@+id/imageView7"
            app:layout_constraintEnd_toStartOf="@+id/textView12"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="24dp"
            android:src="@drawable/rectangle_396"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.509"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="5dp"
            android:src="@drawable/rectangle_397"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintStart_toStartOf="@+id/imageView7" />


        <TextView
            android:id="@+id/textView8"
            android:layout_width="207sp"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/poppins"
            android:text="@string/refer_xyz_to_a_friend_amp_get_40_ecash"
            android:textColor="@color/black"
            android:textFontWeight="500"
            android:textSize="20sp"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/benefits"
            android:fontFamily="@font/poppins"
            android:textColor="@color/black"
            android:textFontWeight="300"
            android:textSize="12sp"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="2dp"
            android:layout_height="2dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/ellipse_8"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="304dp"
            android:layout_height="36dp"
            android:layout_marginStart="3dp"
            android:fontFamily="@font/poppins"
            android:text="@string/you_get_an_ecash_of_40_when_they_take_the_first_ride"
            android:textColor="@color/black"
            android:textFontWeight="400"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/imageView11"
            app:layout_constraintTop_toBottomOf="@+id/textView9" />

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="2dp"
            android:layout_height="2dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="34dp"
            android:src="@drawable/ellipse_8"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toBottomOf="@+id/imageView11" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="304dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:text="@string/they_get_an_ecash_of_20"
            android:fontFamily="@font/poppins"
            android:textColor="@color/black"
            android:textFontWeight="400"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/imageView12"
            app:layout_constraintTop_toBottomOf="@+id/textView10" />

        <Button
            android:id="@+id/button18"
            android:layout_width="140dp"
            android:layout_height="50dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="10dp"
            android:background="@drawable/unlock"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/whats_app"
            android:textAllCaps="false"
            android:textColor="@color/ligth"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintEnd_toEndOf="@+id/imageView10" />

        <Button
            android:id="@+id/button22"
            android:layout_width="140dp"
            android:layout_height="50dp"
            android:layout_marginStart="4dp"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="10dp"

            android:background="@drawable/unlock"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/share_via"
            android:textAllCaps="false"
            android:textColor="@color/ligth"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView7"
            app:layout_constraintEnd_toStartOf="@+id/button18"
            app:layout_constraintStart_toStartOf="@+id/imageView10" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>